<% if @call_finish.errors.any? %>
  <div id="error_explanation">
    <%= t 'general.form_error'%>
  </div>
<% end %>

<%= form_for(@call_finish) do |f| %>

  <div class="field">
    <%= f.label :id_call %><br>
    <%= f.number_field :id_call %>
    <%= error_tag @call_finish, :id_call %>
  </div>
  <div class="field">
    <%= f.label :id_user %><br>
    <%= f.number_field :id_user %>
    <%= error_tag @call_finish, :id_user %>
  </div>
  <div class="field">
    <%= f.label :observation %><br>
    <%= f.text_area :observation %>
    <%= error_tag @call_finish, :observation %>
  </div>
  <div class="field">
    <%= f.label :active %><br>
    <%= f.check_box :active %>
    <%= error_tag @call_finish, :active %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>

<% end %>
